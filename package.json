{
  "private": true,
  "name": "carefree-antd-root",
  "version": "1.0.44",
  "scripts": {
    "clean": "lerna clean --yes",
    "build:ui": "npm run build && npm run copy:md",
    "build": "father-build",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version",
    "publish": "lerna publish",
    "release": "npm run build && npm publish",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "copy:md": "npm run copy:md:lib && npm run copy:md:es",
    "copy:md:lib": "lerna exec -- npm run copy:md:lib && npm run copy:md:es",
    "copy:md:es": "lerna exec -- npm run copy:md:es ",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:update": "umi-test --updateSnapshot",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@umijs/fabric": "^2.10.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@umijs/test": "^3.5.20",
    "dumi": "^1.1.40",
    "father-build": "^1.21.2",
    "gh-pages": "^3.0.0",
    "lerna": "4.0.0",
    "lint-staged": "11.2.3",
    "antd": "^4.17.4",
    "prettier": "2.4.1",
    "react-test-renderer": "17.0.2",
    "umi-request": "^1.4.0",
    "yorkie": "2.0.0",
    "@types/react": "~17.0.43",
    "@types/react-dom": "~17.0.14",
    "copyfiles": "2.4.1"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}