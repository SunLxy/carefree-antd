{
  "private": true,
  "name": "carefree-antd-root",
  "version": "1.0.42",
  "scripts": {
    "init": "rm -rf node_modules && npm install && npm run clean && npm run bootstrap && npm run build",
    "build:ui": "npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "father-build",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version",
    "publish": "lerna publish",
    "release": "npm run build && npm publish",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:update": "umi-test --updateSnapshot",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/fabric": "^2.10.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "react": "17.0.2",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "sortablejs": "1.14.0",
    "umi-request": "1.4.0",
    "query-string": "7.0.1",
    "carefree-utils": "~1.0.15",
    "carefree-antd-form": "~1.0.34",
    "carefree-antd-edit-table": "~1.0.34",
    "carefree-pro-table": "~1.0.34"
  },
  "devDependencies": {
    "@types/immutability-helper": "2.6.3",
    "@types/lodash": "4.14.172",
    "@types/sortablejs": "1.10.7",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@umijs/test": "3.5.20",
    "dumi": "^1.1.40",
    "father-build": "^1.21.2",
    "gh-pages": "^3.0.0",
    "lerna": "4.0.0",
    "lint-staged": "11.2.3",
    "antd": "4.17.4",
    "prettier": "2.4.1",
    "react-test-renderer": "17.0.2",
    "yorkie": "2.0.0",
    "@types/react": "^17.0.28"
  }
}